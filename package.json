{
  "name": "rooksgc-dev-server",
  "version": "0.0.1",
  "description": "Express server for API calls",
  "repository": {
    "type": "git",
    "url": "https://github.com/rooksgc/rooksgc-dev-server"
  },
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "author": "Shestakov Anton<rooksgc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "config": "^3.3.3",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
