{
  "name": "rooksgc-dev-server",
  "version": "0.0.1",
  "description": "Express server for API calls",
  "repository": {
    "type": "git",
    "url": "https://github.com/rooksgc/rooksgc-dev-server"
  },
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "db:migrate:dev": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "db:migrate:prod": "cross-env NODE_ENV=production sequelize-cli db:migrate",
    "db:seeds:dev": "cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "db:seeds:prod": "cross-env NODE_ENV=production sequelize-cli db:seed:all",
    "db:create": "sequelize-cli db:create",
    "db:g:seed": "sequelize-cli seed:generate --seed-name"
  },
  "author": "Shestakov Anton<rooksgc@gmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": 1,
      "@typescript-eslint/no-inferrable-types": [
        "warn",
        {
          "ignoreParameters": true
        }
      ],
      "@typescript-eslint/no-unused-vars": "warn"
    }
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express-jwt": "^6.0.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/nodemailer": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.18",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
